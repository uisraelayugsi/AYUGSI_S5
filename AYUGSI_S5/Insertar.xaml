<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AYUGSI_S5.Insertar">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Insertar "  FontSize="20" HorizontalTextAlignment="Center" />
            <Entry x:Name="txtCodigo" Placeholder="Codigo" Keyboard="Numeric"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre" />
            <Entry x:Name="txtApelldio" Placeholder="Apelliod" />
            <Entry x:Name="txtEdad" Placeholder="Edad" />

            <Button x:Name="btnInsertar" Text="INSERTAR" Clicked="btnInsertar_Clicked"/>
            

        </StackLayout>
    </ContentPage.Content>
</ContentPage>